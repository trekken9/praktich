#include <stdio.h>

int main() {
    double t1, t2, t3, combined_rate, total_time;

    
    printf("Введіть три значення:\n");
    scanf("%lf %lf %lf", &t1, &t2, &t3);

    
    combined_rate = (1.0 / t1) + (1.0 / t2) + (1.0 / t3);

    
    total_time = 1.0 / combined_rate;

    
    printf("Час, необхідний для з'їдання пирога: %.2f годин\n", total_time);

    return 0;
}
